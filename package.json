{
  "name": "virtual-office-uri-avatar-position",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:analyze": "vue-tsc && vite build --mode analyze",
    "build:production": "NODE_ENV=production vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "server": "node server.js",
    "server:production": "NODE_ENV=production node server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "start": "concurrently \"npm run server:production\" \"npm run preview\"",
    "lint": "vue-tsc --noEmit",
    "optimize": "vite-bundle-analyzer dist/stats.html"
  },
  "dependencies": {
    "@types/three": "^0.178.0",
    "dompurify": "^3.2.6",
    "three": "^0.178.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.0.0",
    "vite": "^5.4.19",
    "vue-tsc": "^3.0.1",
    "ws": "^8.18.3"
  }
}
